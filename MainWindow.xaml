<Window x:Class="FoulzExternal.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:FoulzExternal"
        xmlns:explorer="clr-namespace:FoulzExternal.features.games.universal.explorer"
        xmlns:playerlist="clr-namespace:FoulzExternal.features.games.universal.playerlist"
        Title="AXIOM X" Height="650" Width="850" 
        WindowStartupLocation="CenterScreen" 
        ResizeMode="NoResize" 
        Background="Transparent" 
        AllowsTransparency="True" 
        WindowStyle="None"
        Loaded="startvibing">

    <Window.Resources>
        <local:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>

        <Color x:Key="AccentColor">#FFFFFFFF</Color>
        <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}"/>

        <Style x:Key="GlowClose" TargetType="Button">
            <Setter Property="Background" Value="#151515"/>
            <Setter Property="Foreground" Value="#666"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Border x:Name="b" Background="{TemplateBinding Background}" CornerRadius="3">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter TargetName="b" Property="Background" Value="#202020"/>
                                <Setter Property="Foreground" Value="White"/>
                                <Trigger.EnterActions>
                                    <BeginStoryboard>
                                        <Storyboard>
                                            <ColorAnimation Storyboard.TargetProperty="(Button.Foreground).(SolidColorBrush.Color)" To="White" Duration="0:0:0.2"/>
                                            <DoubleAnimation Storyboard.TargetName="b" Storyboard.TargetProperty="Opacity" To="0.8" Duration="0:0:0.2"/>
                                        </Storyboard>
                                    </BeginStoryboard>
                                </Trigger.EnterActions>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="tabz_cuh" TargetType="RadioButton">
            <Setter Property="Height" Value="45"/>
            <Setter Property="Foreground" Value="#666"/>
            <Setter Property="Background" Value="Transparent"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="RadioButton">
                        <Border Background="{TemplateBinding Background}" BorderBrush="{DynamicResource AccentBrush}" BorderThickness="0" x:Name="TabBorder">
                            <Grid>
                                <Rectangle x:Name="AccentLine" HorizontalAlignment="Left" Width="3" Fill="{DynamicResource AccentBrush}" Visibility="Collapsed"/>
                                <ContentPresenter HorizontalAlignment="Left" VerticalAlignment="Center" Margin="20,0,0,0"/>
                            </Grid>
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsChecked" Value="True">
                                <Setter Property="Foreground" Value="White"/>
                                <Setter TargetName="AccentLine" Property="Visibility" Value="Visible"/>
                                <Setter Property="Background" Value="#0D0D0D"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="toggle_vibe" TargetType="ToggleButton">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="ToggleButton">
                        <Border x:Name="Bg" Background="#111" CornerRadius="1" BorderBrush="#222" BorderThickness="1">
                            <TextBlock x:Name="Txt" Text="{TemplateBinding Content}" FontSize="9" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#555" FontWeight="Bold"/>
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsChecked" Value="True">
                                <Setter TargetName="Bg" Property="BorderBrush" Value="{DynamicResource AccentBrush}"/>
                                <Setter TargetName="Txt" Property="Foreground" Value="{DynamicResource AccentBrush}"/>
                                <Setter TargetName="Bg" Property="Effect">
                                    <Setter.Value>
                                        <DropShadowEffect Color="{DynamicResource AccentColor}" BlurRadius="8" ShadowDepth="0" Opacity="0.9"/>
                                    </Setter.Value>
                                </Setter>
                            </Trigger>
                            <Trigger Property="IsChecked" Value="False">
                                <Setter TargetName="Bg" Property="BorderBrush" Value="#222"/>
                                <Setter TargetName="Txt" Property="Foreground" Value="#555"/>
                                <Setter TargetName="Bg" Property="Effect" Value="{x:Null}"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="slider_chill" TargetType="Slider">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Slider">
                        <Grid>
                            <Border Height="2" Background="#1A1A1A" VerticalAlignment="Center"/>
                            <Track x:Name="PART_Track">
                                <Track.Thumb>
                                    <Thumb>
                                        <Thumb.Template>
                                            <ControlTemplate>
                                                <Border Width="12" Height="12" Background="{DynamicResource AccentBrush}" CornerRadius="0" BorderBrush="Black" BorderThickness="1"/>
                                            </ControlTemplate>
                                        </Thumb.Template>
                                    </Thumb>
                                </Track.Thumb>
                            </Track>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="axiom_btn_bruh" TargetType="Button">
            <Setter Property="Background" Value="#111"/>
            <Setter Property="Foreground" Value="#AAA"/>
            <Setter Property="Padding" Value="6,4"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Border Background="{TemplateBinding Background}" BorderBrush="#222" BorderThickness="1" CornerRadius="2">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Background" Value="#151515"/>
                                <Setter Property="Foreground" Value="White"/>
                                <Setter Property="BorderBrush" Value="{DynamicResource AccentBrush}"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="small_btn" TargetType="Button" BasedOn="{StaticResource axiom_btn_bruh}">
            <Setter Property="Width" Value="90"/>
            <Setter Property="Height" Value="26"/>
            <Setter Property="FontSize" Value="12"/>
            <Setter Property="Padding" Value="8,4"/>
        </Style>

        <Style x:Key="combo_chill" TargetType="ComboBox">
            <Setter Property="Background" Value="#0A0A0A"/>
            <Setter Property="Foreground" Value="#DDD"/>
            <Setter Property="BorderBrush" Value="#222"/>
            <Setter Property="BorderThickness" Value="1"/>
            <Setter Property="Padding" Value="6,3"/>
            <Setter Property="HorizontalContentAlignment" Value="Right"/>
            <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="ComboBox">
                        <Grid>
                            <Border x:Name="Bd"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    CornerRadius="3">
                                <DockPanel LastChildFill="True" Margin="4,0">
                                    <ContentPresenter x:Name="ContentSite"
                                      ContentTemplate="{TemplateBinding SelectionBoxItemTemplate}"
                                      Content="{TemplateBinding SelectionBoxItem}"
                                      VerticalAlignment="Center"
                                      HorizontalAlignment="Right"
                                      Margin="0,0,4,0"/>
                                    <ToggleButton x:Name="ToggleButton"
                                          DockPanel.Dock="Right"
                                          Width="28"
                                          Focusable="False"
                                          IsChecked="{Binding IsDropDownOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                                          Background="Transparent"
                                          BorderThickness="0"
                                          Padding="0"
                                          HorizontalAlignment="Right"
                                          OverridesDefaultStyle="True"
                                          FocusVisualStyle="{x:Null}">
                                        <ToggleButton.Template>
                                            <ControlTemplate TargetType="ToggleButton">

                                                <Border x:Name="TbRoot"
                                              Background="Transparent"
                                              Width="{TemplateBinding Width}"
                                              Height="{TemplateBinding Height}"
                                              CornerRadius="2">

                                                    <Grid>
                                                        <Path x:Name="Arrow"
                                                      Data="M 0 0 L 4 4 L 8 0 Z"
                                                      Fill="{DynamicResource AccentBrush}"
                                                      Stretch="Uniform"
                                                      HorizontalAlignment="Center"
                                                      VerticalAlignment="Center"/>
                                                    </Grid>
                                                </Border>
                                                <ControlTemplate.Triggers>
                                                    <Trigger Property="IsMouseOver" Value="True">
                                                        <Setter TargetName="TbRoot" Property="Background" Value="#202020"/>
                                                        <Setter TargetName="Arrow" Property="Fill" Value="White"/>
                                                    </Trigger>
                                                    <Trigger Property="IsPressed" Value="True">
                                                        <Setter TargetName="TbRoot" Property="Background" Value="#222"/>
                                                        <Setter TargetName="Arrow" Property="Fill" Value="White"/>
                                                    </Trigger>
                                                    <Trigger Property="IsChecked" Value="True">
                                                        <Setter TargetName="TbRoot" Property="Background" Value="#161616"/>
                                                        <Setter TargetName="Arrow" Property="Fill" Value="{DynamicResource AccentBrush}"/>
                                                    </Trigger>
                                                </ControlTemplate.Triggers>
                                            </ControlTemplate>
                                        </ToggleButton.Template>
                                    </ToggleButton>
                                </DockPanel>
                            </Border>

                            <Popup x:Name="Popup"
                                   Placement="Bottom"
                                   IsOpen="{TemplateBinding IsDropDownOpen}"
                                   AllowsTransparency="True"
                                   Focusable="False"
                                   PopupAnimation="Fade"
                                   MinWidth="{Binding ActualWidth, RelativeSource={RelativeSource TemplatedParent}}"
                                   StaysOpen="False">
                                <Border Background="#0A0A0A" BorderBrush="#222" BorderThickness="1" CornerRadius="3" Padding="4">
                                    <ScrollViewer CanContentScroll="True" HorizontalScrollBarVisibility="Disabled">
                                        <StackPanel IsItemsHost="True"/>
                                    </ScrollViewer>
                                </Border>
                            </Popup>
                        </Grid>

                        <ControlTemplate.Triggers>
                            <Trigger Property="IsEnabled" Value="False">
                                <Setter TargetName="Bd" Property="Opacity" Value="0.5"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>

            <Setter Property="ItemContainerStyle">
                <Setter.Value>
                    <Style TargetType="ComboBoxItem">
                        <Setter Property="Background" Value="Transparent"/>
                        <Setter Property="Foreground" Value="#DDD"/>
                        <Setter Property="Padding" Value="6,6"/>
                        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
                        <Setter Property="Template">
                            <Setter.Value>
                                <ControlTemplate TargetType="ComboBoxItem">
                                    <Border x:Name="Bd" Background="{TemplateBinding Background}" Padding="{TemplateBinding Padding}">
                                        <ContentPresenter />
                                    </Border>
                                    <ControlTemplate.Triggers>
                                        <Trigger Property="IsMouseOver" Value="True">
                                            <Setter TargetName="Bd" Property="Background" Value="#0F0F0F"/>
                                        </Trigger>
                                        <Trigger Property="IsSelected" Value="True">
                                            <Setter TargetName="Bd" Property="Background" Value="#1E1E1E"/>
                                            <Setter Property="Foreground" Value="White"/>
                                        </Trigger>
                                    </ControlTemplate.Triggers>
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </Setter.Value>
            </Setter>
        </Style>

        <Storyboard x:Key="FadeInSequence">
            <DoubleAnimation Storyboard.TargetName="MainContent" Storyboard.TargetProperty="Opacity" From="0" To="1" Duration="0:0:0.5"/>
        </Storyboard>

        <Storyboard x:Key="PulseGlow" RepeatBehavior="Forever" AutoReverse="True">
            <DoubleAnimation Storyboard.TargetName="OuterGlow" Storyboard.TargetProperty="BlurRadius" From="8" To="18" Duration="0:0:2.8" />
            <DoubleAnimation Storyboard.TargetName="OuterGlow" Storyboard.TargetProperty="Opacity" From="0.08" To="0.28" Duration="0:0:2.8" />
        </Storyboard>

        <Storyboard x:Key="InlineStatusFadeIn"/>
    </Window.Resources>

    <Border Background="#080808" BorderBrush="#1A1A1A" BorderThickness="1" CornerRadius="6" Margin="10">
        <Border.Effect>
            <DropShadowEffect x:Name="OuterGlow" BlurRadius="12" Color="{DynamicResource AccentColor}" Opacity="0.18" ShadowDepth="0"/>
        </Border.Effect>
        <Grid x:Name="MainContent">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="220"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>

            <Border Grid.Column="0" Background="#050505" BorderBrush="#151515" BorderThickness="0,0,1,0" CornerRadius="6,0,0,6">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>

                    <StackPanel Grid.Row="0" Margin="20,25,20,20">
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="foulzers" FontSize="26" FontWeight="Black" Foreground="White"/>
                            <TextBlock Text="C#" FontSize="26" FontWeight="Black" Foreground="{DynamicResource AccentBrush}" Margin="5,0,0,0"/>
                        </StackPanel>
                        <TextBlock Text="👅OPEN SOURCE C# EXTERNAL 👅" FontSize="9" Foreground="#444" FontWeight="Bold" Margin="2,-2,0,0"/>
                    </StackPanel>

                    <StackPanel Grid.Row="1">
                        <RadioButton x:Name="AimbotTab" Style="{StaticResource tabz_cuh}" IsChecked="True">
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="🎯" Margin="0,0,8,0" FontSize="14"/>
                                <TextBlock Text="AIMBOT" VerticalAlignment="Center"/>
                            </StackPanel>
                        </RadioButton>
                        <RadioButton x:Name="SilentTab" Style="{StaticResource tabz_cuh}">
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="🤫" Margin="0,0,8,0" FontSize="14"/>
                                <TextBlock Text="SILENT" VerticalAlignment="Center"/>
                            </StackPanel>
                        </RadioButton>
                        <RadioButton x:Name="VisualsTab" Style="{StaticResource tabz_cuh}">
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="👁️" Margin="0,0,8,0" FontSize="14"/>
                                <TextBlock Text="VISUALS" VerticalAlignment="Center"/>
                            </StackPanel>
                        </RadioButton>
                        <RadioButton x:Name="ChecksTab" Style="{StaticResource tabz_cuh}">
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="✅" Margin="0,0,8,0" FontSize="14"/>
                                <TextBlock Text="CHECKS" VerticalAlignment="Center"/>
                            </StackPanel>
                        </RadioButton>
                        <RadioButton x:Name="MiscTab" Style="{StaticResource tabz_cuh}">
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="🧰" Margin="0,0,8,0" FontSize="14"/>
                                <TextBlock Text="MISC" VerticalAlignment="Center"/>
                            </StackPanel>
                        </RadioButton>
                        <RadioButton x:Name="ExplorerTab" Style="{StaticResource tabz_cuh}">
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="📁" Margin="0,0,8,0" FontSize="14"/>
                                <TextBlock Text="EXPLORER" VerticalAlignment="Center"/>
                            </StackPanel>
                        </RadioButton>
                        <RadioButton x:Name="PlayersTab" Style="{StaticResource tabz_cuh}">
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="👥" Margin="0,0,8,0" FontSize="14"/>
                                <TextBlock Text="PLAYERS" VerticalAlignment="Center"/>
                            </StackPanel>
                        </RadioButton>
                        <RadioButton x:Name="SettingsTab" Style="{StaticResource tabz_cuh}">
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="⚙️" Margin="0,0,8,0" FontSize="14"/>
                                <TextBlock Text="SETTINGS" VerticalAlignment="Center"/>
                            </StackPanel>
                        </RadioButton>
                    </StackPanel>

                    <Border Grid.Row="2" Background="#030303" Padding="12" CornerRadius="4,4,6,6">
                        <StackPanel>
                            <Border Background="#080808" BorderBrush="#111" BorderThickness="1" Padding="8" Margin="0,0,0,10" CornerRadius="3">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Ellipse x:Name="ProfileEllipse" Width="30" Height="30" Fill="#111" Stroke="#222"/>
                                    <StackPanel Grid.Column="1" Margin="10,0,0,0" VerticalAlignment="Center">
                                        <TextBlock x:Name="ProfileNameText" Text="USER" FontSize="10" Foreground="White" FontWeight="Bold"/>
                                        <TextBlock Text="AUTHENTICATED" FontSize="8" Foreground="#444"/>
                                    </StackPanel>
                                </Grid>
                            </Border>

                            <Button Content="ATTACH" Click="hookitup" Style="{StaticResource axiom_btn_bruh}" Height="30" Margin="0,0,0,10" FontWeight="Bold"/>

                            <StackPanel x:Name="InlineStatusWrap">
                                <Grid Margin="0,0,0,5">
                                    <StackPanel Orientation="Horizontal">
                                        <Ellipse x:Name="StatusIndicator" Width="6" Height="6" Fill="#333" Margin="0,0,8,0"/>
                                        <TextBlock x:Name="StatusText" Text="IDLE" Foreground="#444" FontSize="9" FontWeight="Bold"/>
                                    </StackPanel>
                                    <TextBlock x:Name="PidValueText" Text="N/A" FontSize="9" Foreground="#333" HorizontalAlignment="Right"/>
                                </Grid>

                                <UniformGrid Columns="3" Margin="0,5">
                                    <Ellipse x:Name="CachesIndicator" Width="8" Height="8" Fill="#111" Stroke="#222" ToolTip="Caches"/>
                                    <Ellipse x:Name="StorageIndicator" Width="8" Height="8" Fill="#111" Stroke="#222" ToolTip="Storage"/>
                                    <Ellipse x:Name="VmmIndicator" Width="8" Height="8" Fill="#111" Stroke="#222" ToolTip="VMM"/>
                                </UniformGrid>

                                <TextBlock x:Name="VersionValueText" Text="VERSION: N/A" FontSize="8" Foreground="#222" HorizontalAlignment="Center" Margin="0,5"/>
                                <TextBlock x:Name="BaseValueText" Text="0x0000000000000000" FontSize="8" Foreground="#222" HorizontalAlignment="Center" FontFamily="Consolas"/>
                            </StackPanel>
                        </StackPanel>
                    </Border>
                </Grid>
            </Border>

            <Grid Grid.Column="1" Margin="25">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>

                <Button Grid.Row="0" Content="✕" HorizontalAlignment="Right" Click="bye" Style="{StaticResource GlowClose}" Cursor="Hand" Width="34" Height="28" Margin="0,3,3,3">
                    <Button.Effect>
                        <DropShadowEffect BlurRadius="10" Color="{DynamicResource AccentColor}" Opacity="0.6" ShadowDepth="0"/>
                    </Button.Effect>
                </Button>

                <ScrollViewer Grid.Row="1" VerticalScrollBarVisibility="Hidden">
                    <Grid>
                        <StackPanel Visibility="{Binding IsChecked, ElementName=AimbotTab, Converter={StaticResource BooleanToVisibilityConverter}}">
                            <TextBlock Text="AIMBOT" Foreground="#333" FontSize="10" FontWeight="Bold" Margin="0,0,0,15"/>

                            <UniformGrid Columns="2">
                                <Border Background="#0A0A0A" BorderBrush="#111" BorderThickness="1" Padding="12" Margin="0,0,5,10" CornerRadius="4">
                                    <Grid>
                                        <TextBlock Text="AIMBOT" Foreground="#888" FontSize="10" FontWeight="SemiBold"/>
                                        <ToggleButton x:Name="AimbotToggle" HorizontalAlignment="Right" Width="45" Height="22" Style="{StaticResource toggle_vibe}" Content="OFF" Click="aimbottgl"/>
                                    </Grid>
                                </Border>

                                <Border Background="#0A0A0A" BorderBrush="#111" BorderThickness="1" Padding="12" Margin="5,0,0,10" CornerRadius="4">
                                    <Grid>
                                        <TextBlock Text="TOGGLE TYPE" Foreground="#888" FontSize="10" FontWeight="SemiBold"/>
                                        <ComboBox x:Name="ToggleTypeCombo" HorizontalAlignment="Right" Width="120" Style="{StaticResource combo_chill}" SelectionChanged="toggletype">
                                            <ComboBoxItem Content="Hold"/>
                                            <ComboBoxItem Content="Toggle"/>
                                        </ComboBox>
                                    </Grid>
                                </Border>
                            </UniformGrid>

                            <UniformGrid Columns="2">
                                <Border Background="#0A0A0A" BorderBrush="#111" BorderThickness="1" Padding="12" Margin="0,0,5,10" CornerRadius="4">
                                    <StackPanel>
                                        <TextBlock Text="AIM TYPE" Foreground="#888" FontSize="10" FontWeight="SemiBold"/>
                                        <ComboBox x:Name="AimingTypeCombo" HorizontalAlignment="Right" Width="120" Style="{StaticResource combo_chill}" SelectionChanged="aimingtype">
                                            <ComboBoxItem Content="Camera"/>
                                            <ComboBoxItem Content="Mouse"/>
                                        </ComboBox>
                                    </StackPanel>
                                </Border>

                                <Border Background="#0A0A0A" BorderBrush="#111" BorderThickness="1" Padding="12" Margin="5,0,0,10" CornerRadius="4">
                                    <StackPanel>
                                        <TextBlock Text="TARGET BONE" Foreground="#888" FontSize="10" FontWeight="SemiBold"/>
                                        <ComboBox x:Name="TargetBoneCombo" HorizontalAlignment="Right" Width="120" Style="{StaticResource combo_chill}" SelectionChanged="targetbone">
                                            <ComboBoxItem Content="Head"/>
                                            <ComboBoxItem Content="HumanoidRootPart"/>
                                            <ComboBoxItem Content="Left Arm"/>
                                            <ComboBoxItem Content="Right Arm"/>
                                            <ComboBoxItem Content="Left Leg"/>
                                            <ComboBoxItem Content="Right Leg"/>
                                        </ComboBox>
                                    </StackPanel>
                                </Border>
                            </UniformGrid>

                            <UniformGrid Columns="2">
                                <Border Background="#0A0A0A" BorderBrush="#111" BorderThickness="1" Padding="12" Margin="0,0,5,10" CornerRadius="4">
                                    <Grid>
                                        <TextBlock Text="KEYBIND" Foreground="#888" FontSize="10" FontWeight="SemiBold"/>
                                        <Button x:Name="AimbotKeyButton" HorizontalAlignment="Right" Width="120" Style="{StaticResource small_btn}" Click="aimbotkey">SET</Button>
                                    </Grid>
                                </Border>

                                <Border Background="#0A0A0A" BorderBrush="#111" BorderThickness="1" Padding="12" Margin="5,0,0,10" CornerRadius="4">
                                    <Grid>
                                        <TextBlock Text="STICKY AIM" Foreground="#888" FontSize="10" FontWeight="SemiBold"/>
                                        <ToggleButton x:Name="StickyAimToggle" HorizontalAlignment="Right" Width="45" Height="22" Style="{StaticResource toggle_vibe}" Content="OFF" Click="stickyaim"/>
                                    </Grid>
                                </Border>
                            </UniformGrid>

                            <Border Background="#0A0A0A" BorderBrush="#111" BorderThickness="1" Padding="12" Margin="0,0,0,10" CornerRadius="4">
                                <StackPanel>
                                    <TextBlock Text="SENSITIVITY" Foreground="#888" FontSize="10" FontWeight="SemiBold"/>
                                    <Slider x:Name="SensitivitySlider" Style="{StaticResource slider_chill}" Minimum="0.1" Maximum="5" Value="1" ValueChanged="sensitivity" Margin="0,5,45,0"/>
                                    <TextBlock x:Name="SensitivityValueText" Text="1.0" Foreground="{DynamicResource AccentBrush}" HorizontalAlignment="Right" FontSize="10" FontWeight="Bold"/>
                                </StackPanel>
                            </Border>

                            <UniformGrid Columns="2">
                                <Border Background="#0A0A0A" BorderBrush="#111" BorderThickness="1" Padding="12" Margin="0,0,5,10" CornerRadius="4">
                                    <StackPanel>
                                        <TextBlock Text="SMOOTHNESS" Foreground="#888" FontSize="10" FontWeight="SemiBold"/>
                                        <ToggleButton x:Name="SmoothnessToggle" HorizontalAlignment="Right" Width="45" Height="22" Style="{StaticResource toggle_vibe}" Content="OFF" Click="smoothness"/>
                                        <Slider x:Name="SmoothnessXSlider" Style="{StaticResource slider_chill}" Minimum="0" Maximum="1" Value="0" ValueChanged="smoothx" Margin="0,8,45,0"/>
                                        <Slider x:Name="SmoothnessYSlider" Style="{StaticResource slider_chill}" Minimum="0" Maximum="1" Value="0.05" ValueChanged="smoothy" Margin="0,8,45,0"/>
                                    </StackPanel>
                                </Border>

                                <Border Background="#0A0A0A" BorderBrush="#111" BorderThickness="1" Padding="12" Margin="5,0,0,10" CornerRadius="4">
                                    <StackPanel>
                                        <TextBlock Text="PREDICTION" Foreground="#888" FontSize="10" FontWeight="SemiBold"/>
                                        <ToggleButton x:Name="PredictionToggle" HorizontalAlignment="Right" Width="45" Height="22" Style="{StaticResource toggle_vibe}" Content="OFF" Click="prediction"/>
                                        <Slider x:Name="PredictionXSlider" Style="{StaticResource slider_chill}" Minimum="0" Maximum="4" Value="2" ValueChanged="predx" Margin="0,8,45,0"/>
                                        <Slider x:Name="PredictionYSlider" Style="{StaticResource slider_chill}" Minimum="0" Maximum="4" Value="2" ValueChanged="predy" Margin="0,8,45,0"/>
                                    </StackPanel>
                                </Border>
                            </UniformGrid>

                            <UniformGrid Columns="2">
                                <Border Background="#0A0A0A" BorderBrush="#111" BorderThickness="1" Padding="12" Margin="0,0,5,10" CornerRadius="4">
                                    <StackPanel>
                                        <TextBlock Text="Aimbot FOV" Foreground="#888" FontSize="10" FontWeight="SemiBold"/>
                                        <Slider x:Name="AimbotFovSlider" Style="{StaticResource slider_chill}" Minimum="10" Maximum="1000" Value="100" ValueChanged="aimbotfov" Margin="0,5,45,0"/>
                                        <TextBlock x:Name="AimbotFovValueText" Text="100" Foreground="{DynamicResource AccentBrush}" HorizontalAlignment="Right" FontSize="10" FontWeight="Bold"/>
                                    </StackPanel>
                                </Border>
                                <Border Background="#0A0A0A" BorderBrush="#111" BorderThickness="1" Padding="12" Margin="5,0,0,10" CornerRadius="4">
                                    <StackPanel>
                                        <TextBlock Text="RANGE" Foreground="#888" FontSize="10" FontWeight="SemiBold"/>
